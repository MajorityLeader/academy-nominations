{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-restricted-syntax": "off",
        "no-await-in-loop": "off",
        "no-console": "off",
        "no-continue": "off",
        "import/extensions": "off",
        "max-len": "off",
        "radix": "off"
    }
}
